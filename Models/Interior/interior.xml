<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>cockpit-separator</object-name>
    <object-name>cockpit-tub.001</object-name>
    <object-name>cockpit-tub.001</object-name>
    <object-name>cockpit-tub-step</object-name>
    <object-name>intcockpit</object-name>
    <object-name>intcockpit.001</object-name>
    <object-name>interieur.001</object-name>
    <object-name>interieur_bench</object-name>
    <object-name>intporteDB</object-name>
    <object-name>intporteDH</object-name>
    <object-name>intporteGB</object-name>
    <object-name>intporteGH</object-name>
    <object-name>intportepilote</object-name>
    <object-name>inttour</object-name>
    <object-name>medpack.001</object-name>
    <object-name>o2-bottle</object-name>
    <object-name>panel-hood.001</object-name>
    <object-name>panel-hood.002</object-name>
    <object-name>planchet</object-name>
    <object-name>rotorbreak-lever</object-name>
    <object-name>engine-lever.001</object-name>
    <object-name>collective-stick.001</object-name>
    <object-name>console.001</object-name>
    <object-name>console.002</object-name>

    <!-- Objets transparents ALS -->
    <object-name>intvitres</object-name>
    <object-name>intvitresG</object-name>
    <object-name>intvitresD</object-name>
    <object-name>intvitrepilote</object-name>

    <!-- Objets transparents HDR-->
    <object-name>intvitres.hdr</object-name>
    <object-name>intvitresG.hdr</object-name>
    <object-name>intvitresD.hdr</object-name>
    <object-name>intvitrepilote.hdr</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
     <object-name>medpack.001</object-name>
     <object-name>o2-bottle</object-name>
     <object-name>rotorbreak-lever</object-name>
	</animation>

  <!-- GLASS ALS -->
  <effect>
    <inherits-from>../Effects/Glass/glassrain</inherits-from>
    <object-name>intvitres</object-name>
    <object-name>intvitresG</object-name>
    <object-name>intvitresD</object-name>
    <object-name>intvitrepilote</object-name>
  </effect>

 <!-- GLASS HDR -->
  <effect>
    <inherits-from>../Effects/Glass/glass</inherits-from>
    <object-name>intvitres.hdr</object-name>
    <object-name>intvitresG.hdr</object-name>
    <object-name>intvitresD.hdr</object-name>
    <object-name>intvitrepilote.hdr</object-name>
  </effect>

  <model>
    <path>Panel/all-panels.xml</path>
  </model>

  <model>
    <path>../Pilot/all-pilots.xml</path>
  </model>

  <model>
    <path>Seat/all-seats.xml</path>
  </model>

  <model>
    <path>Panel/Instruments/gear/gear-panel.xml</path>
  </model>

  <!-- <model>
    <path>Panel/Instruments/right/all-rights.xml</path>
  </model> -->

  <model>
    <path>Panel/Instruments/raduga/all-radugas.xml</path>
  </model>

  <model>
    <path>Panel/Instruments/fan/all-fans.xml</path>
  </model>

  <model>
    <path>Cyclic/all-cyclics.xml</path>
  </model>

  <!-- SWITCH GLASS -->

  <animation>
    <type>select</type>
    <object-name>intvitres</object-name>
    <object-name>intvitresG</object-name>
    <object-name>intvitresD</object-name>
    <object-name>intvitrepilote</object-name>
    <condition>
      <not><property>/sim/rendering/hdr/hdr-enabled</property></not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>intvitres.hdr</object-name>
    <object-name>intvitresG.hdr</object-name>
    <object-name>intvitresD.hdr</object-name>
    <object-name>intvitrepilote.hdr</object-name>
    <condition>
      <property>/sim/rendering/hdr/hdr-enabled</property>
    </condition>
  </animation>

  <!-- Porte pilote -->
  <animation>
    <name>IntLaPortePilote</name>
    <object-name>intportepilote</object-name>
    <object-name>intvitrepilote</object-name>
    <object-name>intportepilote.hdr</object-name>
    <object-name>intvitrepilote.hdr</object-name>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>IntLaPortePilote</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>doors.crew.toggle();</script>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>IntLaPortePilote</object-name>
    <property>sim/model/door-positions/crew/position-norm</property>
    <factor> -100 </factor>
    <axis>
      <x1-m> -5.021 </x1-m>
      <y1-m>  0.867 </y1-m>
      <z1-m> -1.019 </z1-m>
      <x2-m> -5.021 </x2-m>
      <y2-m>  0.805 </y2-m>
      <z2-m> -0.836 </z2-m>
    </axis>
  </animation>

  <!-- Portes de la soute -->
  <animation>
    <name>IntPorteCargoGauche</name>
    <object-name>intporteGH</object-name>
    <object-name>intvitresG</object-name>
    <object-name>intporteGH.hdr</object-name>
    <object-name>intvitresG.hdr</object-name>
  </animation>

  <animation>
    <name>IntPorteCargoDroite</name>
    <object-name>intporteDH</object-name>
    <object-name>intvitresD</object-name>
    <object-name>intporteDH.hdr</object-name>
    <object-name>intvitresD.hdr</object-name>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>IntPorteCargoGauche</object-name>
    <object-name>intporteGB</object-name>
    <object-name>IntPorteCargoDroite</object-name>
    <object-name>intporteDB</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>doors.passenger.toggle();</script>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>IntPorteCargoGauche</object-name>
    <property>sim/model/door-positions/passenger/position-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>    0 </dep></entry>
      <entry><ind> 1 </ind><dep> -108 </dep></entry>
    </interpolation>
    <axis>
      <x1-m> -3.837 </x1-m>
      <y1-m> -0.875 </y1-m>
      <z1-m> -0.546 </z1-m>
      <x2-m> -3.294 </x2-m>
      <y2-m> -0.879 </y2-m>
      <z2-m> -0.546 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>intporteGB</object-name>
    <property>sim/model/door-positions/passenger/position-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>   0 </dep></entry>
      <entry><ind> 1 </ind><dep> 115 </dep></entry>
    </interpolation>
    <axis>
      <x1-m> -3.837 </x1-m>
      <y1-m> -0.822 </y1-m>
      <z1-m> -1.770 </z1-m>
      <x2-m> -3.294 </x2-m>
      <y2-m> -0.831 </y2-m>
      <z2-m> -1.770 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>IntPorteCargoDroite</object-name>
    <property>sim/model/door-positions/passenger/position-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>    0 </dep></entry>
      <entry><ind> 1 </ind><dep> -108 </dep></entry>
    </interpolation>
    <axis>
      <x1-m> -3.294 </x1-m>
      <y1-m>  0.879 </y1-m>
      <z1-m> -0.546 </z1-m>
      <x2-m> -3.837 </x2-m>
      <y2-m>  0.875 </y2-m>
      <z2-m> -0.546 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>intporteDB</object-name>
    <property>sim/model/door-positions/passenger/position-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>   0 </dep></entry>
      <entry><ind> 1 </ind><dep> 115 </dep></entry>
    </interpolation>
    <axis>
      <x1-m> -3.294 </x1-m>
      <y1-m>  0.831 </y1-m>
      <z1-m> -1.770 </z1-m>
      <x2-m> -3.837 </x2-m>
      <y2-m>  0.822 </y2-m>
      <z2-m> -1.770 </z2-m>
    </axis>
  </animation>

  <!-- Collective -->

  <animation>
   <type>rotate</type>
   <object-name>collective-stick.001</object-name>
   <property>/controls/engines/engine/throttle</property>
   <offset-deg>15</offset-deg>
   <factor>-20</factor>
   <center>
    <x-m>-5.21597</x-m>
    <y-m>-0.443104</y-m>
    <z-m>-1.5363</z-m>
   </center>
   <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
   </axis>
  </animation>

  <!-- Engine levers -->

  <animation>
   <type>rotate</type>
   <object-name>engine-lever.001</object-name>
   <property>sim/model/mi24/engine</property>
   <interpolation>
      <entry><ind>  0.00 </ind><dep> 0 </dep></entry>
      <entry><ind>  1.03 </ind><dep> 65 </dep></entry>
  </interpolation>
   <center>
    <x-m>-4.88769</x-m>
    <y-m>-0.445449</y-m>
    <z-m>-1.48379 m</z-m>
   </center>
   <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
   </axis>
  </animation>

</PropertyList>
