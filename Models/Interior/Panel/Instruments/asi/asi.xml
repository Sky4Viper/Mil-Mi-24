<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>asi.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
    <object-name>visses</object-name>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
    <object-name>vitre</object-name>
  </animation>

  <!-- working for Compositor -->
  <effect>
    <inherits-from>../../../../Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
    <condition>
      <property>controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>controls/lighting/luminosite-lights</factor-prop>
      <!-- <red>0.8</red>
      <green>0.3</green>
      <blue>0.1</blue> -->

      <red>1.0</red>
      <green>0.94</green>
      <blue>0.75</blue>

    </emission>
  </animation>

<!--
Airspeed Needle
km/h to kts
50 = 26.9978
100 = 53.9957
150 = 80.9935
200 = 107.991
250 = 134.989
300 = 161.987
350 = 188.985
400 = 215.983
450 = 242.981
-->

  <animation>
    <type>rotate</type>
    <object-name>needle1</object-name>
    <property>velocities/airspeed-kt</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0.0 </dep></entry>
      <entry><ind> 26.9978 </ind><dep>  5.66 </dep></entry>
      <entry><ind> 53.9957 </ind><dep> 53.5 </dep></entry>
      <entry><ind> 80.9935 </ind><dep> 93.4 </dep></entry>
      <entry><ind> 107.991 </ind><dep> 135.0 </dep></entry>
      <entry><ind> 134.989 </ind><dep> 177.0 </dep></entry>
      <entry><ind> 161.987 </ind><dep> 220.0 </dep></entry>
      <entry><ind> 188.985 </ind><dep> 260.0 </dep></entry>
      <entry><ind> 215.983 </ind><dep> 300.0 </dep></entry>
      <entry><ind> 242.981 </ind><dep> 338.0 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>



</PropertyList>
