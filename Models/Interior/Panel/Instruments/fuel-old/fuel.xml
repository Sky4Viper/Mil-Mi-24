<?xml version="1.0"?>

<PropertyList>

  <path>fuel.ac</path>

  <!-- Internal instruments lighting -->
  <animation>
    <type>material</type>
    <object-name>Needle1</object-name>
    <object-name>Face</object-name>
    <condition>
      <property>controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>   0.50 </red>
      <green> 0.40 </green>
      <blue>  0.30 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Needle1</object-name>
    <property>consumables/fuel/total-fuel-lbs</property>
    <!--
    <offset-deg>-189</offset-deg>
      <max>1960</max>
    <factor>0.13</factor>
    -->
    <interpolation>
      <entry><ind>    0.0 </ind><dep> -189 </dep></entry>
      <entry><ind>  442.0 </ind><dep> -124 </dep></entry>
      <entry><ind>  662.0 </ind><dep>  -97 </dep></entry>
      <entry><ind> 1545.0 </ind><dep>   14 </dep></entry>
      <entry><ind> 1820.0 </ind><dep>   65 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
