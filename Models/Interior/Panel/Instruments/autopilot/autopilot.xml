    </condition>
    <emission>
      <red>   1.6 </red>
      <green> 0.0 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>

  <animation>
    <type>select</type>
    <object-name>autopilot-light-on.001</object-name>
    <name>HOVER-ON</name>
    <condition>
      <equals>
        <property>/autopilot/locks/heading</property>
        <value type="string">true-heading-hold</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>autopilot-light-on.003</object-name>
    <name>ROUTE-ON</name>
    <condition>
      <equals>
        <property>/autopilot/locks/heading</property>
        <value type="string">nav1-hold</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>autopilot-light-on.005</object-name>
    <name>R-ALT-OFF</name>
    <condition>
      <not-equals>
        <property>/autopilot/locks/altitude</property>
        <value type="string">agl-hold</value>
      </not-equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>autopilot-light-on.006</object-name>
