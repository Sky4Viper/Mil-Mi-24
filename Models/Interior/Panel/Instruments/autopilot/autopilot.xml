    <object-name>autopilot-btn.009</object-name>
    <object-name>autopilot-btn.010</object-name>
    <condition>
      <property>controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>   0.1 </red>
      <green> 0.1 </green>
      <blue>  0.1 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>autopilot-face.001</object-name>
    <condition>
      <property>controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>   0.9 </red>
      <green> 0.8 </green>
      <blue>  0.8 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>autopilot-light-on.001</object-name>
    <object-name>autopilot-light-on.003</object-name>
    <object-name>autopilot-light-on.006</object-name>
    <object-name>autopilot-light-on.007</object-name>
    <object-name>autopilot-light-on.008</object-name>
    <object-name>autopilot-light-on.009</object-name>
    <object-name>autopilot-light-on.013</object-name>
    <condition>
      <property>controls/electric/battery-switch</property>
    </condition>
    <emission>
      <red>   0.0 </red>
      <green> 1.6 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>autopilot-light-on.002</object-name>
    <object-name>autopilot-light-on.004</object-name>
    <object-name>autopilot-light-on.005</object-name>
