  <!-- autopilot/settings/target-agl-ft -->

  <animation>
    <type>knob</type>
    <object-name>autopilot-trim-knob.006</object-name>
    <increase>
      <binding>
        <command>property-adjust</command>
        <property>autopilot/settings/target-agl-ft</property>
        <step>20.0</step>
        <min>0</min>
        <max>1000</max>
        <wrap>false</wrap>
      </binding>
      <binding>
        <command>nasal</command>
        <script>switchclicksound.click("dial")</script>
      </binding>
    </increase>
    <decrease>
      <binding>
        <command>property-adjust</command>
        <property>autopilot/settings/target-agl-ft</property>
        <step>-20.0</step>
        <min>0</min>
        <max>1000</max>
        <wrap>false</wrap>
      </binding>
      <binding>
        <command>nasal</command>
        <script>switchclicksound.click("dial")</script>
      </binding>
    </decrease>
    <hovered>
      <binding>
          <command>set-tooltip</command>
          <tooltip-id>adf2-kHz</tooltip-id>
