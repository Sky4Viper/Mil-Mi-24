<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>fuel.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
    <object-name>visses</object-name>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
    <object-name>vitre</object-name>
  </animation>

  <!-- working for Compositor -->
  <effect>
    <inherits-from>../../../../Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
    <condition>
      <property>controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>controls/lighting/luminosite-lights</factor-prop>
      <!-- <red>0.8</red>
      <green>0.3</green>
      <blue>0.1</blue> -->

      <red>1.0</red>
      <green>0.94</green>
      <blue>0.75</blue>

    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle1</object-name>
    <property>consumables/fuel/total-fuel-lbs</property>
  <interpolation>
	  <entry><ind>0.0</ind><dep>-189</dep></entry>
    <entry><ind>442.0</ind><dep>-124</dep></entry>
    <entry><ind>662.0</ind><dep>-97</dep></entry>
    <entry><ind>1545</ind><dep>14</dep></entry>
	  <entry><ind>1820</ind><dep>65</dep></entry>
  </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
