  <animation>
      <type>pick</type>
      <object-name>lft-switch.030</object-name>
      <action>
        <button>0</button>
        <button>1</button>
        <repeatable>false</repeatable>
        <interval-sec>0.1</interval-sec>
        <binding>
          <command>property-toggle</command>
          <property>controls/electric/fan</property>
        </binding>
        <binding>
         <command>nasal</command>
         <script>switchclicksound.click("switch3")</script>
        </binding>
      </action>
      <hovered>
       <binding>
        <command>set-tooltip</command>
         <tooltip-id>switch-fan</tooltip-id>
         <label>Fan Toggle</label>
       </binding>
      </hovered>
  </animation>

