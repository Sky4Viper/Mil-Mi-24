<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>panel-left.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>left-panel-base</object-name>
    <object-name>left-switches-base</object-name>
    <object-name>lft-button.001</object-name>
    <object-name>lft-button.002</object-name>
    <object-name>lft-button.003</object-name>

    <object-name>lft-knob.001</object-name>
    <object-name>lft-knob.002</object-name>

    <object-name>lft-light-frame.004</object-name>

    <object-name>lft-switch.001</object-name>
    <object-name>lft-switch.002</object-name>
    <object-name>lft-switch.003</object-name>
    <object-name>lft-switch.004</object-name>
    <object-name>lft-switch.005</object-name>
    <object-name>lft-switch.006</object-name>
    <object-name>lft-switch.007</object-name>
    <object-name>lft-switch.021</object-name>

    <object-name>lft-switch-cover.001</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>left-panel-base</object-name>
    <object-name>left-switches-base</object-name>
    <object-name>lft-button.001</object-name>
    <object-name>lft-button.002</object-name>
    <object-name>lft-button.003</object-name>

    <object-name>lft-knob.001</object-name>
    <object-name>lft-knob.002</object-name>

    <object-name>lft-light-frame.004</object-name>

    <object-name>lft-switch.001</object-name>
    <object-name>lft-switch.002</object-name>
    <object-name>lft-switch.003</object-name>
    <object-name>lft-switch.004</object-name>
    <object-name>lft-switch.005</object-name>
    <object-name>lft-switch.006</object-name>
    <object-name>lft-switch.007</object-name>
    <object-name>lft-switch.021</object-name>

    <object-name>lft-switch-cover.001</object-name>
  </animation>

  <animation>
      <type>material</type>
      <object-name>left-switches-base</object-name>
      <object-name>lft-light-frame.004</object-name>
      <object-name>lft-button.001</object-name>
      <object-name>lft-button.002</object-name>
      <object-name>lft-button.003</object-name>
      <object-name>lft-switch.001</object-name>
      <object-name>lft-switch.002</object-name>
      <object-name>lft-switch.003</object-name>
      <object-name>lft-switch.004</object-name>
      <object-name>lft-switch.005</object-name>
      <object-name>lft-switch.006</object-name>
      <object-name>lft-switch.007</object-name>
      <object-name>lft-switch.021</object-name>
      <object-name>lft-switch-cover.001</object-name>
      <emission>
        <red>0.1</red>
        <green>0.1</green>
        <blue>0.1</blue>
        <factor-prop>controls/lighting/luminosite-lights</factor-prop>
      </emission>
      <condition>
        <property>controls/electric/battery-switch</property>
      </condition>
  </animation>

</PropertyList>
