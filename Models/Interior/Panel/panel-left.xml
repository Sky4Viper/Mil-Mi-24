    <!-- Left Inner standby frequency (1 kHz) knob -->
    <animation>
        <type>knob</type>
        <object-name>ark1-knob.002</object-name>
        <property alias="/params/diala-1-khz"/>
        <factor>-3.6</factor>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>-5.45771</x-m>
            <y-m>-0.55022</y-m>
            <z-m>-1.14848</z-m>
        </center>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/instrumentation/adf[0]/frequencies/standby-khz</property>
                <factor>1</factor>
                <min>200</min>
                <max>1800</max>
                <resolution>1</resolution>
                <wrap>true</wrap>
            </binding>
            <binding>
                <command>property-adjust</command>
                <property alias="/params/diala-1-khz"/>
                <factor>3.6</factor>
                <min>0</min>
                <max>360</max>
                <wrap>true</wrap>
            </binding>
            <binding>
                <command>nasal</command>
                <script>switchclicksound.click("dial")</script>
            </binding>
            <binding>
                <command>property-assign</command>
                <property>instrumentation/adf[0]/frequencies/selected-khz</property>
                <property>instrumentation/adf[0]/frequencies/standby-khz</property>
                <condition>
                  <not>
                    <property>controls/instrumentation/adf-chanel</property>
                  </not>
                </condition>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>adf-kHz</tooltip-id>
                <label>Adjust ADF frequency by 1 kHz %4.1f</label>
                <property>/instrumentation/adf[0]/frequencies/standby-khz</property>
            </binding>
        </hovered>
    </animation>

