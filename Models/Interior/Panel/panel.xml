<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>separation</object-name>
    <object-name>fond</object-name>
    <object-name>panel</object-name>
    <object-name>map</object-name>
    <object-name>boutons</object-name>
  </animation>

  <!-- AI -->
  <model>
    <path>Instruments/ai/ai.xml</path>
    <offsets>
      <x-m> -0.018 </x-m>
      <y-m> -0.040 </y-m>
      <z-m>  0.230 </z-m>
    </offsets>
  </model>

  <!-- AI backup -->
  <model>
    <path>Instruments/ai2/ai.xml</path>
    <offsets>
      <x-m> 0.000 </x-m>
      <y-m> 0.090 </y-m>
      <z-m> 0.160 </z-m>
    </offsets>
  </model>

  <!-- ALT -->
  <model>
    <path>Instruments/alt/alt.xml</path>
    <offsets>
      <x-m>  0.002 </x-m>
      <y-m> -0.160 </y-m>
      <z-m>  0.160 </z-m>
    </offsets>
  </model>

  <!-- ASI -->
  <model>
    <path>Instruments/asi/asi.xml</path>
    <offsets>
