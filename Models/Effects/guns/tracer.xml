<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Red Tracer -->
<PropertyList>

  <path>tracer.ac</path>

  <animation>
    <type>material</type>
    <emission>
      <red>   2 </red>
      <green> 1 </green>
      <blue>  0 </blue>
    </emission>
  </animation>

  <animation>
    <enable-hot type="bool">false</enable-hot>
  </animation>

  <animation>
    <type>dist-scale</type>
    <object-name>tracer</object-name>
    <factor>0.0008</factor>
    <offset>0</offset>
  </animation>

  <animation>
    <type>material</type>
    <object-name>tracer</object-name>
    <emission>
      <factor>1</factor>
      <offset>1</offset>
      <red>   1 </red>
      <green> 0.333 </green>
      <blue>  0.333 </blue>
    </emission>
  </animation>

</PropertyList>
