<?xml version="1.0"?>

<PropertyList>

	<path>missile.ac</path>

    <animation>
     <object-name>9M120-mis</object-name>
	 <object-name>flame-one</object-name>
	 <object-name>flame-two</object-name>
    </animation>

    <model>
      <name>smoke</name>
      <path>9m120-smoke.xml</path>
     <offsets>
      <x-m>   5.26 </x-m>
      <y-m>  -0.08 </y-m>
      <z-m>  -0.09 </z-m>
     </offsets>
    </model>
	
	<!--  Rotate flame -->
	
	<effect>
    		<inherits-from>Effects/model-transparent</inherits-from>
    		<condition>
      		<property>/sim/rendering/rembrandt/enabled</property>
    		</condition>
    		<object-name>flame-one</object-name>
    		<object-name>flame-two</object-name>
 	</effect>
	
	<animation>
		<type>material</type>
		<object-name>flame-one</object-name>
    		<object-name>flame-two</object-name>
		<emission>
			<red>255</red>
			<green>255</green>
			<blue>254</blue>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>flame-one</object-name>
		<property>/sim/time/elapsed-sec</property>
		<factor>-30</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>flame-two</object-name>
		<!-- <object-name>smoke</object-name> -->
		<property>/sim/time/elapsed-sec</property>
		<factor>40</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>
