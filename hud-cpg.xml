<!-- AUTOPILOT -->

 <label>
  <name>Autopilot Altitude Hold</name>
  <x>197</x>
  <y>0</y>
  <width>1</width>
  <height>1</height>
  <box>true</box>
  <format>%04d</format>
  <halign>left</halign>
