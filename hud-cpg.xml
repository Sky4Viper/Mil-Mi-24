<!-- Weapon stations -->

<label>
  <name>9M120 Selected</name>
  <x>-100</x>
  <y>150</y>
  <width>1</width>
  <height>1</height>
  <format>9M120-ATAKA x1</format>
  <halign>right</halign>
  <condition>
   <and>
    <property>/controls/armament/master-arm</property>
    <or>
     <and>
      <equals>
      <property>controls/armament/weapon-type</property>
      <value>3</value>
      </equals>
      <equals>
      <property>controls/armament/station[0]/release-stick</property>
      <value>0</value>
      </equals>
     </and>
     <and>
      <equals>
       <property>controls/armament/weapon-type</property>
       <value>4</value>
      </equals>
      <equals>
       <property>controls/armament/station[1]/release-stick</property>
       <value>0</value>
      </equals>
     </and>
     <and>
      <equals>
       <property>controls/armament/weapon-type</property>
       <value>5</value>
      </equals>
      <equals>
       <property>controls/armament/station[2]/release-stick</property>
       <value>0</value>
      </equals>
      </and>
      <and>
     <equals>
      <property>controls/armament/weapon-type</property>
      <value>6</value>
     </equals>
     <equals>
      <property>controls/armament/station[3]/release-stick</property>
      <value>0</value>
      </equals>
     </and>
    </or>
   </and>
  </condition>
</label>

