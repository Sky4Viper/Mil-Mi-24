<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <lighting>
    <instrument-lights type="bool">false</instrument-lights>
    <nav-lights        type="bool">false</nav-lights>
    <taxi-lights       type="bool">false</taxi-lights>
    <beacon            type="bool">false</beacon>
    <strobe            type="bool">false</strobe>
    <landing-lights    type="bool">false</landing-lights>
    <luminosite-lights type="float">1</luminosite-lights>
    <floods-lights     type="float">1</floods-lights>
    <elt               type="bool">false</elt>
  </lighting>

  <APU>
    <master            type="bool">true</master>
    <battery           type="double">28.0</battery>
    <running           type="bool">true</running>
    <off-start-run     type="int">1</off-start-run>
  </APU>

  <engines>
    <engine            n="0">
      <boost           type="double">1</boost>
      <throttle        type="double">0</throttle>
    </engine>
    <engine            n="1">
      <boost           type="double">1</boost>
      <throttle        type="double">0</throttle>
    </engine>
  </engines>

  <electric>
    <battery-switch    type="bool">false</battery-switch>
    <generator>0</generator>
    <alimentation>
      <deice           type="int">0</deice>
    </alimentation>
    <engines>
      <engine          n="0">
        <fuel-pump>0</fuel-pump>
      </engine>
      <engine          n="1">
        <fuel-pump>0</fuel-pump>
      </engine>
    </engines>
  </electric>

  <rotor>
    <reltarget         type="double">1</reltarget>
    <maxreltorque      type="double">1</maxreltorque>
    <brake             type="bool">true</brake>
  </rotor>

  <!-- ################ -->
  <armament>
    <trigger type="bool">false</trigger>
    <station n="0">
     <release-stick type="bool">false</release-stick>
    </station>
    <station n="1">
     <release-stick type="bool">false</release-stick>
    </station>
    <station n="2">
     <release-stick type="bool">false</release-stick>
    </station>
    <station n="3">
     <release-stick type="bool">false</release-stick>
    </station>
  </armament>
  <!-- ################ -->

  <gear>
    <brake-parking     type="bool">1</brake-parking>
    <emergency         type="bool">false</emergency>
  </gear>

  <flight>
    <rudder-trim       type="double"> 0.00 </rudder-trim>
    <aileron-trim      type="double"> 0.00 </aileron-trim>
    <elevator-trim     type="double"> 0.00 </elevator-trim>
  </flight>

</PropertyList>
