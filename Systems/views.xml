<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <chase-distance-m type="double" archive="y">-25</chase-distance-m>

  <view n="100">
    <name>Copilot-Gunner View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y"> 0.00 </x-offset-m> <!--right-->
      <y-offset-m archive="y"> -1.1650 </y-offset-m> <!--up-->
      <z-offset-m archive="y"> -6.868 </z-offset-m> <!--back-->
      <pitch-offset-deg type="double">-16</pitch-offset-deg>
      <default-field-of-view-deg type="double">105</default-field-of-view-deg>
      <dynamic-view type="bool">true</dynamic-view>
      <limits>
					<left>
						<heading-max-deg>-100</heading-max-deg>
						<x-offset-threshold-deg>0</x-offset-threshold-deg>
						<x-offset-max-m>0</x-offset-max-m>
					</left>
					<right>
						<heading-max-deg>100</heading-max-deg>
						<x-offset-threshold-deg>0</x-offset-threshold-deg>
						<x-offset-max-m>0</x-offset-max-m>
					</right>
					<enabled>1</enabled>
			</limits>
    </config>
  </view>

  <view n="101">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  1.801 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 10.637 </z-offset-m> <!--Back-->
      <pitch-offset-deg type="double">-5</pitch-offset-deg>
      <default-field-of-view-deg type="double">30</default-field-of-view-deg>
    </config>
  </view>

  <view n="0">
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y">  0.000 </x-offset-m> <!--right-->
      <y-offset-m archive="y"> -0.5217 </y-offset-m> <!--up-->
      <z-offset-m archive="y"> -5.535 </z-offset-m> <!--back-->
      <pitch-offset-deg type="double">-10</pitch-offset-deg>
      <default-field-of-view-deg type="double">108</default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

</PropertyList>
